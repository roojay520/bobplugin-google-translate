{
  "identifier": "com.roojay.bobplugin-google-translate",
  "category": "translate",
  "version": "0.7.0",
  "name": "Google 翻译",
  "summary": "Bob google 翻译插件",
  "author": "roojay <roojay520@gmail.com>",
  "appcast": "https://github-raw.roojay.com/roojay520/bobplugin-google-translate/master/src/appcast.json",
  "homepage": "https://github.com/roojay520/bobplugin-google-translate",
  "icon": "114",
  "minBobVersion": "0.5.0",
  "options": [
    {
      "identifier": "cache",
      "type": "menu",
      "title": "缓存",
      "defaultValue": "enable",
      "menuValues": [
        {
          "title": "开启",
          "value": "enable"
        },
        {
          "title": "关闭",
          "value": "disable"
        }
      ]
    },
    {
      "identifier": "version",
      "type": "menu",
      "title": "接口版本",
      "defaultValue": "tmp",
      "menuValues": [
        {
          "title": "WEB(模拟调用网页版接口, 翻译结果和官网一致)",
          "value": "web"
        },
        {
          "title": "RPC(直接调用 RPC 接口, 段落翻译不太准确)",
          "value": "rpc"
        }
      ]
    },
    {
      "identifier": "proxyApi",
      "type": "text",
      "title": "代理域名",
      "desc": "如果你的网络环境需要代理才能访问 Google Translate, 可以在此填写代理域名, 格式形如: google-translate.proxy.com",
      "defaultValue": ""
    },
    {
      "identifier": "stringFmt",
      "type": "menu",
      "title": "字符串分隔",
      "defaultValue": "disable",
      "desc": "拆分驼峰/中划线/下划线格式的长字符串, 例如: fooBarBaz-intoString → foo Bar Baz - into String",
      "menuValues": [
        {
          "title": "开启",
          "value": "enable"
        },
        {
          "title": "关闭",
          "value": "disable"
        }
      ]
    }
  ]
}
